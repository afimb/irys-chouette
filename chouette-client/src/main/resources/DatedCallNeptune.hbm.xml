<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="net.dryade.siri.chouette.client.model.DatedCallNeptune" table="datedcall">
    <composite-id>
      <key-property name="datedVehicleJourneyId"><column name="datedVehicleJourneyId"/></key-property>
      <key-property name="stopPointNeptuneRef"><column name="stoppointid"/></key-property>
    </composite-id>
    <property name="expectedDepartureTime" type="calendar"/>
    <property name="expectedArrivalTime" type="calendar"/>
    <property name="departureStatus" type="net.dryade.siri.chouette.client.hibernate.VisitStatusUserType" column="status"/>
    <!-- <property name="arrivalStatus" type="net.dryade.siri.chouette.client.hibernate.VisitStatusUserType"/> -->
  </class>
</hibernate-mapping>
