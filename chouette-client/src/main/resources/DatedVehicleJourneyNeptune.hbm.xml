<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="net.dryade.siri.chouette.client.model.DatedVehicleJourneyNeptune" table="datedvehiclejourney">
    <id name="id" type="long"  unsaved-value="null" >
      <column name="id">
        <comment>Internal identification</comment>
      </column>
      <generator class="identity"/>
    </id>
    <property column="objectId" name="datedVehicleJourneyNeptuneRef" type="string" not-null="true"/>
    <property column="application_date" name="originAimedDepartureTime" type="calendar" not-null="true"/>
  </class>
</hibernate-mapping>
